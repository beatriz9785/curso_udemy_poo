import java.util.Scanner;

/*
 receber um número n que vai ser as vezes que o programa vai repetir o loop
 receber números para somar.
 */

public class Main {
    
    public static void main(String[] args){
        
        
        Scanner sc = new Scanner(System.in);
        
        int n = sc.nextInt();
        int soma = 0;
        
        for(int i = 0; i < n; i++){
            int x = sc.nextInt();
            soma += x;
        }
        System.out.println("a soma é: "+soma);
        sc.close();
    }
    
}
