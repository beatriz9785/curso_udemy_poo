/**
 *
 * @author Beatriz
 */

public class Main {
    
    public static void main(String[] args){
        
        
        Scanner sc = new Scanner(System.in);
        
        int n1 = sc.nextInt();
        int n2 = sc.nextInt();
        
        if(n1 % n2 ==0 || n2 % n1 == 0){
            System.out.println("os números são múltiplos.");
        }
        else{
            System.out.println("Os números não são múltiplos.");
        }
        
        
        sc.close(); //pra que serve isso mesmo? ...
        
    }
    
}
